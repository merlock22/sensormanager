<%= semantic_form_for @sensor do |f| %>
  <%= f.inputs do %>
    <%= f.input :concentrador, :as => :select, :member_label => :nombre %>
    <%= f.input :nombre %>
    <%= f.input :descripcion %>
    <%= f.input :estado %>
    <%= f.input :parametro %>
    <%= f.input :valor %>
    <%= f.input :uds %>
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>
